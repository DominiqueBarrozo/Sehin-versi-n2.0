@model IEnumerable<Sehin_versión2._0.Models.Trabajadores>

@{
    ViewBag.Title = "Index";
}

<h2>Listado de trabajadores</h2>

<p>
    @Html.ActionLink("Dar de alta trabajador", "Create")
</p>
<table class="table">
    <tr>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
